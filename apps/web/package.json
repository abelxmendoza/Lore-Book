{
  "name": "web",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/vercel-build-diagnostics.js",
    "build": "vite build --logLevel info",
    "postbuild": "node scripts/post-build-validation.js",
    "build:verbose": "vite build --logLevel verbose",
    "preview": "vite preview",
    "lint": "eslint src",
    "format": "prettier --write src",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage --config vitest.config.coverage.ts",
    "test:coverage:check": "vitest run --coverage --config vitest.config.coverage.ts && npm run test:coverage:threshold",
    "test:coverage:threshold": "node scripts/check-coverage-threshold.js",
    "test:unit": "vitest run",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:build": "node scripts/build-validation.test.js",
    "test:chunk-deps": "node scripts/chunk-dependency-check.js",
    "test:black-screen": "vitest run --config vitest.integration.config.ts src/**/*.integration.test.tsx src/**/*.integration.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:production": "playwright test e2e/production-build.spec.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:black-screen && npm run test:e2e && npm run test:cypress",
    "test:pre-deploy": "npm run test:unit && npm run test:black-screen && npm run build && npm run test:build && npm run test:chunk-deps"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@remix-run/router": "^1.23.2",
    "@sentry/react": "^10.26.0",
    "@sentry/tracing": "^7.120.4",
    "@stripe/react-stripe-js": "^5.4.0",
    "@stripe/stripe-js": "^8.5.2",
    "@supabase/supabase-js": "^2.46.1",
    "@tanstack/react-virtual": "^3.5.0",
    "@vitejs/plugin-react": "^4.3.1",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dompurify": "^3.3.0",
    "highlight.js": "^11.11.1",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.439.0",
    "posthog-js": "^1.297.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-force-graph-2d": "^1.29.0",
    "react-hook-form": "^7.51.5",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.3",
    "react-simple-code-editor": "^0.14.1",
    "reactflow": "^11.11.4",
    "recharts": "^2.12.7",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^2.3.0",
    "vis-data": "^8.0.3",
    "vis-timeline": "^8.5.0",
    "vite": "^5.4.1",
    "zustand": "^4.5.4"
  },
  "overrides": {
    "preact": "^10.28.2",
    "esbuild": "^0.25.0",
    "tailwindcss": "3.4.17"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@testing-library/cypress": "^10.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.7.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "jsdom": "^25.0.1",
    "msw": "^2.12.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.53.1",
    "vitest": "^2.1.8",
    "wait-on": "^9.0.3"
  }
}
